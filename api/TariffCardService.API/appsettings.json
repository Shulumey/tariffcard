{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "NLog": {
    "autoReload": true,
    "throwExceptions": true,
    "shutdownOnDispose": true,
    "default-wrapper": {
      "type": "AsyncWrapper",
      "overflowAction": "Block"
    }
  }
}
